{
    "editor.formatOnSave": true,
    "editor.fontSize": 17,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
        "source.fixAll.tslint": true,
        "source.organizeImports": true
    },
    "javascript.validate.enable": false,
    "typescript.validate.enable": false
}